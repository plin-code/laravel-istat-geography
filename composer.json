{
    "name": "plin-code/laravel-istat-geography",
    "description": "Laravel package for importing and managing Italian geography data from ISTAT",
    "keywords": [
        "PlinCode",
        "laravel",
        "istat",
        "geography",
        "italy",
        "regions",
        "provinces",
        "municipalities",
        "countries"
    ],
    "homepage": "https://github.com/plin-code/laravel-istat-geography",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniele Barbaro",
            "email": "barbaro.daniele@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "league/csv": "^9.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PlinCode\\IstatGeography\\": "src/",
            "PlinCode\\IstatGeography\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PlinCode\\IstatGeography\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PlinCode\\IstatGeography\\IstatGeographyServiceProvider"
            ],
            "aliases": {
                "IstatGeography": "PlinCode\\IstatGeography\\Facades\\IstatGeography"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
